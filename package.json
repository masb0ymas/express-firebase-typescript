{
  "name": "express-firebase-typescript",
  "version": "1.0.0",
  "description": "Express Firebase TypeScript",
  "private": true,
  "scripts": {
    "dev": "nodemon ./src/bin/server.ts",
    "start": "nodemon ./dist/bin/server.js",
    "clean": "rm -rf ./dist",
    "lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "before-build": "yarn && npm run lint && npm run type-check && npm run clean",
    "heroku-prebuild": "yarn && npm run clean",
    "build": "npx tsc -p .",
    "serve:production": "NODE_ENV=production node ./dist/bin/server.js",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major"
  },
  "author": "masb0ymas",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "convert-excel-to-json": "^1.7.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.17.0",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "exceljs": "^4.2.1",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "googleapis": "^68.0.0",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "http-errors": "^1.8.0",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.5.0",
    "nodemailer-mailgun-transport": "^2.0.2",
    "pug": "^3.0.0",
    "rate-limiter-flexible": "^2.2.1",
    "redis": "^3.0.2",
    "request-ip": "^2.1.3",
    "swagger-jsdoc": "^6.0.2",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/convert-excel-to-json": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-useragent": "^1.0.0",
    "@types/hpp": "^0.2.1",
    "@types/http-errors": "^1.8.0",
    "@types/lodash": "^4.14.168",
    "@types/module-alias": "^2.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.27",
    "@types/nodemailer": "^6.4.1",
    "@types/nodemailer-mailgun-transport": "^1.4.2",
    "@types/redis": "^2.8.28",
    "@types/request-ip": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "engines": {
    "node": ">=12.x"
  },
  "_moduleAliases": {
    "@bin/*": "dist/bin/*",
    "@config/*": "dist/config/*",
    "@constants/*": "dist/constants/*",
    "@controllers/*": "dist/controllers/*",
    "@helpers/*": "dist/helpers/*",
    "@interfaces/*": "dist/interfaces/*",
    "@jobs/*": "dist/jobs/*",
    "@middlewares/*": "dist/middlewares/*",
    "@migrations/*": "dist/migrations/*",
    "@models/*": "dist/models/*",
    "@modules/*": "dist/modules/*",
    "@routes/*": "dist/routes/*",
    "@utils/*": "dist/utils/*",
    "@views/*": "dist/views/*"
  }
}
